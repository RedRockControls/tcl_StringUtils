    <Method Name="Inc" Id="{e4194c23-04f5-0360-2f2c-232d18402ffa}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD Inc
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[Count := Count + 1;
IF Count > EndValue THEN
    Reset();
END_IF]]></ST>
      </Implementation>
    </Method>
    <Property Name="NextValue" Id="{a99f870c-8d1e-069d-2f0d-fc9f0c7fe44b}" FolderPath="Properties\">
